{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "mix",
			"task": "Build",
			"problemMatcher": [
				"$mixCompileError",
				"$mixCompileWarning"
			],
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"label": "mix: Build",
			"options": {
				"cwd": "${workspaceFolder}/exins"
			}
		},
		{
			"type": "mix",
			"task": "Build dependencies",
			"problemMatcher": [
				"$mixCompileError",
				"$mixCompileWarning"
			],
			"group": "build",
			"label": "mix: Build dependencies",
			"options": {
				"cwd": "${workspaceFolder}/exins"
			}
		},
		{
			"type": "mix",
			"task": "Get deps",
			"problemMatcher": [
				"$mixCompileError",
				"$mixCompileWarning"
			],
			"label": "mix: Get deps",
			"options": {
				"cwd": "${workspaceFolder}/exins"
			}
		}
	]
}